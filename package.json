{
  "name": "military-tees-uk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage --coverage.provider=v8",
    "test:ci": "vitest run --coverage --coverage.provider=v8 --reporter=verbose",
    "test:component": "vitest run src/components/**/*.test.tsx",
    "test:api": "vitest run src/app/api/**/*.test.ts",
    "test:hooks": "vitest run src/hooks/**/*.test.ts",
    "test:database": "vitest run src/lib/__tests__/database.test.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:journey": "playwright test tests/journeys/",
    "test:all": "npm run test:ci && npm run test:e2e",
    "security:audit": "npm audit --audit-level high",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.1.1",
    "@stripe/react-stripe-js": "^3.9.0",
    "@stripe/stripe-js": "^7.8.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.52.0",
    "algoliasearch": "^5.34.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.6",
    "lucide-react": "^0.525.0",
    "next": "15.4.2",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.10.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "resend": "^4.8.0",
    "stripe": "^18.3.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.7",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.4.2",
    "eslint-plugin-playwright": "^0.20.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jsdom": "^26.1.0",
    "lint-staged": "^15.2.0",
    "msw": "^2.0.11",
    "postcss": "^8.4.32",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:ci"
    }
  }
}
